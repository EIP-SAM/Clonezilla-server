#Debian
# Network Interface, this should match your network
allow-hotplug eth0
iface eth0 inet dhcp

# Interface for Clonezilla, make sure you use a "Class C" IP (192.168.x.x)
iface eth1 inet static
address 192.168.1.2
netmask 255.255.255.0
gateway 192.168.1.1
